sc_agent_of_deleting_sc_element_from_draft
=> nrel_main_idtf:
	[sc-агент удаления sc-элемента из черновика] (* <- lang_ru;; *);
	[sc-agent of deleting sc-element from draft] (* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_deleting_sc_element_from_draft_condition => ..sc_agent_of_deleting_sc_element_from_draft_result);

<= nrel_sc_agent_key_sc_element: 
	{
	question_initiated;
	question;
	action_of_deleting_sc_elements_from_draft
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_deleting_sc_element_from_draft
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_deleting_sc_element_from_draft
		};;

	-> sc_agent_of_deleting_sc_element_from_draft_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_deleting_sc_element_from_draft_condition
= [*
	action_of_deleting_sc_elements_from_draft _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> rrel_1:: .._parameter1;;
	.._question _-> rrel_2:: .._parameter2;;
*];;

..sc_agent_of_deleting_sc_element_from_draft_result
= [*
	action_of_deleting_sc_elements_from_draft _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> rrel_1:: .._parameter1;;
	.._question _-> rrel_2:: .._parameter2;;
*];;
